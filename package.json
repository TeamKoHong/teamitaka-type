{
  "name": "teamitaka-type",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' next dev --port 3000",
    "dev:clean": "rm -rf .next && npm run dev:stable",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "reset": "npm run clean && npm run dev:stable"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "next": "15.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.3",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vitest": "^1.0.4"
  }
}
